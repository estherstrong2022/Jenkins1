pipeline {
    agent any
    stages {
        stage ('init') {
            steps {
                echo 'This is first project'
            }
        }
        stage('parallel'){
            agent {
                docker {
                    image 'maven'
                    step{
                        echo 'install docker maven'
                    }
                }
            }
        }
        stage ('tetiary'){
            steps{
                echo 'mvn --version'
            }
        }
    }
}
