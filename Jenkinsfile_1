pipeline {
    agent any
    stages {
        stage ('front end') {
            agent {
                docker { image '3.9.2-eclipse-temurin-11'}
            }
            steps {
                sh 'mvn --version'
            }
        }
        stage ('backend') {
            agent {
                docker { image '20-alpine3.16'}
            }
        }
    }
}
